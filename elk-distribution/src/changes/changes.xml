<document xmlns="http://maven.apache.org/changes/1.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/changes/1.0.0 http://maven.apache.org/xsd/changes-1.0.0.xsd">
	<properties>
		<title>ELK Reasoner Project</title>
	</properties>
	<body>
		<release version="0.1.1" date="in SVN" description="Subsequent release">
			<action dev="frantisek" type="add">
				Support for object property chain
				inclusion axioms.
			</action>
			<action dev="markus" type="update">
				Re-engineering of ELK OWL object
				hierarchy using interfaces.
			</action>
			<action dev="yevgeny" type="add">
				Support for Protege 4 auto-update
				functionality for ELK Protege plugin.
			</action>
		</release>
		<release version="0.1.0" date="2011-07-19" description="First public release">
			<action dev="yevgeny" type="add">
				Initial public release of ELK
				reasoner.
			</action>
		</release>
	</body>
</document>