Prefix(:=<http://www.semanticweb.org/Test.owl#>)
Prefix(owl:=<http://www.w3.org/2002/07/owl#>)
Prefix(rdf:=<http://www.w3.org/1999/02/22-rdf-syntax-ns#>)
Prefix(xml:=<http://www.w3.org/XML/1998/namespace>)
Prefix(xsd:=<http://www.w3.org/2001/XMLSchema#>)
Prefix(rdfs:=<http://www.w3.org/2000/01/rdf-schema#>)

Ontology(<http://www.semanticweb.org/Test.owl>

EquivalentClasses(:_valid_string_A	DataHasValue(:dp "This is a string!"))
EquivalentClasses(:_valid_string_B	DataHasValue(:dp "Édition française."))
EquivalentClasses(:_valid_string_C	DataHasValue(:dp "AT&amp;T"))
EquivalentClasses(:_valid_string_D	DataHasValue(:dp "12.5"))
EquivalentClasses(:_valid_string_E	DataHasValue(:dp ""))
EquivalentClasses(:_valid_string_F	DataHasValue(:dp "   Отступ 3 пробела   "))
EquivalentClasses(:_valid_string_G	DataHasValue(:dp "Multiline 
									text"))

EquivalentClasses(:AllStrings		DataSomeValuesFrom(:dp xsd:string))
EquivalentClasses(:AnyNonEmptyString	DataSomeValuesFrom(:dp DatatypeRestriction(xsd:string xsd:minLength "1"^^xsd:integer)))
EquivalentClasses(:AnyShortString	DataSomeValuesFrom(:dp DatatypeRestriction(xsd:string xsd:minLength "0"^^xsd:integer xsd:maxLength "5"^^xsd:integer)))
EquivalentClasses(:NumericString	DataSomeValuesFrom(:dp DatatypeRestriction(xsd:string xsd:pattern "[0-9., ]+")))

EquivalentClasses(:InvalidStringA	DataSomeValuesFrom(:dp DatatypeRestriction(xsd:string xsd:maxLength "5"^^xsd:integer xsd:minLength "10"^^xsd:integer)))
EquivalentClasses(:InvalidStringB	DataSomeValuesFrom(:dp DatatypeRestriction(xsd:string xsd:maxLength "-2"^^xsd:integer)))
EquivalentClasses(:InvalidStringC	DataSomeValuesFrom(:dp DatatypeRestriction(xsd:string xsd:minLength "-10"^^xsd:integer)))
)